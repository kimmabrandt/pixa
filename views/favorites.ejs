<div class="mainbody">
  <div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
      <!-- HEADING -->
      <div class="heading">
        <h2>Favorites</h2>
      </div>
      <form method="POST" action="/favorites" id="hideForm">
		    <input type="text" id="userId" name="userId" value="<%- currentUser.id %>">
		    <input type="submit" value="Send">
		  </form> 
        <div class="text-center">
        <h3><%= tagName %>'s Favorites</h3>
        <div class="masonry">
          <% photos.forEach(function(photo){ %>
            <div class='item'>
            <a href="../<%= photo.id %>"><img src=" <%= photo.url %> "></a>
            </div>
          <% }); %>
        </div>
      </div>

    </div>
  </div> 




  		 
